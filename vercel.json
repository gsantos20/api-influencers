{
  "version": 2,
  "builds" : [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }

  ],
  "routes": [
      {
        "src": "(.*)",
        "dest": "dist/server.js"
      }
  ],
  "installCommand": "yarn install --production --frozen-lockfile",
  "buildCommand": "yarn vercel-build"
}
