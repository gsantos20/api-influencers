{
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  },
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "builds" : [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
      {
        "src": "(.*)",
        "dest": "dist/server.js"
      }
  ]

}
